<% layout('layout') -%>

<h2>My Account</h2>

<form>
    <fieldset>
	<div class="control-group">
	    <div class="controls">
		<div class="input-prepend">
		    <span class="add-on"><i class="icon-envelope"></i></span>
		    <input type="text" name="email" value="<%=email%>"/>
		</div>
		<div class="input-prepend">
		    <span class="add-on"><i class="icon-envelope"></i></span>
		    <input type="text" name="fullname" value="<%=fullname%>"/>
		</div>

	    </div>
	</div>
    </fieldset>
</form>
